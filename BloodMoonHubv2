if game.PlaceId == 2788229376 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("BloodMoon Hub", "BloodTheme")
    local Main = Window:NewTab("Main")
    local Fun = Window:NewTab("Fun Stuff")
    local Credits = Window:NewTab("Credits")
    local AutoFarm  Window:NewTab("AutoFarm")
    local Powers = Window:NewTab("Powers")
    -- MAIN SECTION
    local MainSection = Main:NewSection("Main Stuff")
    MainSection:NewButton("Anti Stomp!", "People can't stomp you", function()
        print("UwU!")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/4lrq/4lrq/main/Anti%20Stomp"))()
    end)
    MainSection:NewButton("Fly!", "PRESS M TO USE! Makes you able to fly", function()
        print("UwU!")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/4lrq/4lrq/main/Fly"))()
    end)
    -- AUTOFARM
    local AutoFarmSection = AutoFarm:NewSection("AutoFarm")
    AutoFarmSection:NewButton("AutoFarm", "Autofarm money", function()
        print("OHIO")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/4lrq/4lrq/main/Autofarm"))
    end)
    -- POWERS
    local PowersSection = Powers:NewSection("Powers")
    PowersSection:NewButton("Superhuman!", "Makes your very fast", function()
        print("SuperPower!")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/4lrq/4lrq/main/Superhuman"))()
    end)
    -- FUN SECTION
    local FunSection = Fun:NewSection("Superpowers")
    FunSection:NewButton("Venom!", "Gives you superpower", function()
        print("Venom!")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/4lrq/4lrq/main/Venom2"))()
    end)
    FunSection:NewButton("Wolverine!", "Gives you superpower", function()
        print("Wolverine!")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/4lrq/4lrq/main/Wolverine"))()
    end)
    -- CREDITS
    local CreditsSection = Credits:NewSection("Credits")
    CreditsSection:NewLabel("MADE BY LRQ")
end
